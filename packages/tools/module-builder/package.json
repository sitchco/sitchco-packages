{
    "name": "@sitchco/module-builder",
    "version": "2.1.4",
    "description": "Dynamic Vite build tool for sitchco modules/blocks",
    "type": "module",
    "main": "src/index.js",
    "scripts": {
        "build": "npx depcheck | tee /dev/tty | grep -q \"Missing dependencies\" && exit 1 || exit 0",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "test:watch": "vitest"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@kucrut/vite-for-wp": "catalog:",
        "@sitchco/project-scanner": "workspace:*",
        "chalk": "catalog:",
        "glob": "catalog:",
        "imagemin": "catalog:",
        "imagemin-svgo": "catalog:",
        "laravel-vite-plugin": "catalog:",
        "postcss-custom-media": "catalog:",
        "postcss-nested": "catalog:",
        "rollup-plugin-external-globals": "catalog:",
        "sass-embedded": "catalog:",
        "svgstore": "catalog:",
        "vite": "catalog:",
        "vite-plugin-external": "catalog:",
        "vite-plugin-static-copy": "catalog:"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "@vitest/coverage-v8": "catalog:",
        "@vitest/ui": "catalog:",
        "vitest": "catalog:"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
